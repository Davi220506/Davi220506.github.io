#include<iostream>


using namespace std;

int MaiorNumeroDeUmVetor (int v[5])
{
	int maiorValor;
	for (int i = 0; i < 5; i++)
	{
		if ( i == 0 )
		{
			maiorValor = v[i];
		}
		if (maiorValor < v[i]) maiorValor = v[i];
	}
	  return maiorValor;
}

main()
{
    int v[5];
	int maiorValor;

   for (int i = 0 ; i < 5 ; i ++)
   {
   	  cout << "Insira o valor do vetor: ";
   	  cin >> v[i];
   }
	 maiorValor = MaiorNumeroDeUmVetor(v);
	cout << "O maior numero do vetor e: " << maiorValor ;
}
